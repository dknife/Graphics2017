
# variables
a = 4
b = 3
c = a/b
print("a/b=", c)

a = 1.5
b = 2.5
print(a+b)

# if - else
number = 8
if number%2 is 0 and number>10:
    print(number, "는 10보다 큰 짝수")
elif number%2 is 0 :
    print(number, "는 10보다 작거나 같은 짝수")
else :
    print(number, "는 홀수")
    print("odd")


# for ( add numbers from 1 to 10 )
sumValue = 0
for i in range(1,11) :   # for(int i=1; i<11; i++) {
    print(i)
    sumValue += i

print("sum=", sumValue)


print("hello world")

